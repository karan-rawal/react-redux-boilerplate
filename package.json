{
  "name": "react-redux-boilerplate",
  "version": "2.0.0",
  "description": "A simple boilerplate for react and redux(With thunk) applications. It includes unit testing with jest.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "rimraf ./dist && npm test && webpack",
    "start": "webpack-dev-server",
    "lint": "eslint src --ext .jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karan-rawal/react-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "boilerplate",
    "jest",
    "unit",
    "testing"
  ],
  "author": "Karan Rawal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/karan-rawal/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/karan-rawal/react-redux-boilerplate#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.1",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.0",
    "node-sass": "^4.9.2",
    "rimraf": "^2.6.2",
    "sass": "^1.9.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/test-setup/test-polyfill.js",
      "<rootDir>/test-setup/test-config.js"
    ],
    "moduleNameMapper": {
      "\\.(scss|css)$": "<rootDir>/test-setup/scss-transformer.js",
      "\\.(eot|ttf|woff|woff2|jpg|png|svg)$": "<rootDir>/test-setup/assets-transformer.js"
    }
  }
}
